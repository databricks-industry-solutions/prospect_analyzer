# Databricks Configuration
# Copy this file to .env and fill in your values (if you need them)

# Databricks Workspace Settings
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-personal-access-token

# Unity Catalog Settings
CATALOG_NAME=main
SCHEMA_NAME=prospect_analyzer

# Email Configuration
NOTIFICATION_EMAIL=your-email@company.com

# SQL Warehouse Configuration  
WAREHOUSE_ID=your-warehouse-id

# Processing Configuration
BATCH_SIZE=75
CONTENT_LIMIT=10000

# Note: These environment variables are optional.
# The primary configuration method is through databricks.yml
# Use this file only if you need environment-specific overrides.