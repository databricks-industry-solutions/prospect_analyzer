{
  "datasets": [
    {
      "displayName": "Prospect Campaign Data",
      "name": "prospect_campaign_data",
      "queryLines": [
        "SELECT \n",
        "  CONCAT('<div style=\"font-weight: bold; color: #8B4513;\">', p1.company_name, '</div>',\n",
        "         '<a href=\"', p1.website, '\" target=\"_blank\" style=\"color: #CD853F; text-decoration: none;\">üåê ', p1.website, '</a>') as formatted_header,\n",
        "  p2.composite_score as brick_score,\n",
        "  p2.ai_business_summary,\n",
        "  p3.all_email_links as email_action_buttons,\n",
        "  p1.company_name,\n",
        "  p3.email_generated_at as last_run\n",
        "FROM catalog.schema.customer_list p1 -- CHANGE THIS: Replace with your customer list table\n",
        "left join catalog.schema.v_prospect_analysis p2 -- CHANGE THIS: Replace with your prospect analysis view\n",
        "on p1.id = p2.id\n",
        "left join catalog.schema.v_email_campaigns p3 -- CHANGE THIS: Replace with your email campaigns view\n",
        "on p1.id = p3.id\n",
        "where p2.brick_company_score > 0.0\n",
        "ORDER BY composite_score DESC"
      ]
    }
  ],
